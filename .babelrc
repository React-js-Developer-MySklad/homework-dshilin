{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    ["@babel/preset-react", { "runtime": "automatic"}]
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@src": "./src",
          "@components": "./src/app/components",
          "@utils": "./src/utils",
          "@assets": "./src/assets"
        }
      }
    ]
  ]
}